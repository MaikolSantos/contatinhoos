<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Relat√≥rio</title>
    <style>
      table {
        width: 100%;
      }

      th {
        text-align: left;
      }
    </style>
  </head>
  <body>
    <div>
      <h1>Lista de Contatos</h1>
      <table>
        <thead>
          <tr>
            <th>Nome</th>
            <th>Telefone</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
          <% if (contacts) { %> 
            <% contacts.forEach(contact => { %>
              <tr>
                <td><%= contact.name %></td>
                <td><%= contact.phone %></td>
                <td><%= contact.email %></td>
              </tr>
            <% }); %>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>
